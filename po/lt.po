msgstr "Į_rankinės etiketė :"

#: ../src/nact/nautilus-actions-config-tool.ui.h:89
msgid "Toolti_p :"
msgstr "_Paaiškinimas :"

#: ../src/nact/nautilus-actions-config-tool.ui.h:90
msgid "_Icon :"
